    "queries": {
      "title": "Search queries or URLs",
      "type": "string",
      "description": "Bing Search queries (e.g. food in NYC) and/or full URLs (e.g. https://www.bing.com/search?q=food+NYC). Enter one item per line.",
      "prefill": "Hotels in NYC\nRestaurants in NYC\nhttps://www.bing.com/search?q=restaurants+in+NYC",
      "editor": "textarea",
      "pattern": "[^\\s]+"
  },
  "resultsPerPage": {
      "title": "Results per page",
      "type": "integer",
